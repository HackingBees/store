<html>
  <body>
    <table>
      <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Description</td>
        <td>Suggested Price</td>
      </tr>
      <% for (i=0;i<list.length;i++) {%>
        <tr>
          <td><%=list[i].id%></td>
          <td><%=list[i].name%></td>
          <td><%=list[i].description%></td>
          <td><%=list[i].suggested_price%></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
